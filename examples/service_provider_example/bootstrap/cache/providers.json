{
    "providers": [
        "*providers.ConfigServiceProvider",
        "*providers.LoggerServiceProvider",
        "*providers.ProductServiceProvider",
        "*providers.ContainerServiceProvider",
        "*providers.UserServiceProvider",
        "*providers.ClientServiceProvider",
        "*providers.ClientDeferredServiceProvider",
        "*providers.ClientEventServiceProvider",
        "*main.TestProvider"
    ],
    "eager": [
        "*providers.ConfigServiceProvider",
        "*providers.LoggerServiceProvider",
        "*providers.ProductServiceProvider",
        "*providers.ContainerServiceProvider",
        "*providers.UserServiceProvider",
        "*providers.ClientServiceProvider",
        "*main.TestProvider"
    ],
    "deferred": {
        "client.analytics.service": "*providers.ClientEventServiceProvider",
        "client.deferred.service": "*providers.ClientDeferredServiceProvider",
        "client.event.service": "*providers.ClientEventServiceProvider"
    },
    "when": {
        "*providers.ClientEventServiceProvider": [
            "client.requested",
            "analytics.requested",
            "user.created",
            "application.boot.complete"
        ]
    }
}